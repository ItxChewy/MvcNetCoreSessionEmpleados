@model List<Empleado>

<h1>Session salario Empleados</h1>
<p>
	<a asp-controller="Empleados" asp-action="SumaSalarial">
		Mostar Suma Salarial
	</a>
</p>
<ul class="list-group">
	@foreach (Empleado empleado in Model)
	{
		<li class="list-group-item">
			@empleado.Apellido
			<a asp-controller="Empleados"
			   asp-action="SessionSalarios"
			   asp-route-salario="@empleado.Salario">
				Almacenar salario: @empleado.Salario
			</a>
		</li>
	}
</ul>