@{
	List<int> ids = Context.Session.GetObject<List<int>>("IDSEMPLEADOSV5");
	//List<Empleado> favoritos = Context.RequestServices.GetService<IMemoryCache>().Get<List<Empleado>>("FAVORITOS");
	List<Empleado> favoritos = this.memoryChache.Get<List<Empleado>>("FAVORITOS");
}
<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>@ViewData["Title"] - MvcNetCoreSessionEmpleados</title>
	<script type="importmap"></script>
	<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
	<link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
	<link rel="stylesheet" href="~/MvcNetCoreSessionEmpleados.styles.css" asp-append-version="true" />
</head>
<body>
	<header>
		<nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
			<div class="container-fluid">
				<a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">MvcNetCoreSessionEmpleados</a>
				<button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
						aria-expanded="false" aria-label="Toggle navigation">
					<span class="navbar-toggler-icon"></span>
				</button>
				<div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
					<ul class="navbar-nav flex-grow-1">
						<li class="nav-item">
							<a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
						</li>
						<li class="nav-item">
							<a class="nav-link text-dark" asp-area="" asp-controller="Empleados" asp-action="SessionSalarios">Salarios</a>
						</li>
						<li class="nav-item">
							<a class="nav-link text-dark" asp-controller="Empleados" asp-action="SessionEmpleadosV5">SessionEmpleadoV5</a>
						</li>
						<li class="nav-item">
							<a class="nav-link text-dark" asp-controller="Empleados" asp-action="EmpleadosFavoritos">Favoritos</a>
						</li>
						<li class="nav-item">
							<a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
						</li>
						@if (ids != null)
						{
							<li class="nav-item">
								<h3 style="color:chartreuse">Empleados Session: @ids.Count</h3>
							</li>
						}else{
							<li class="nav-item">
								<h3 style="color:chartreuse">Empleados Session: 0</h3>
							</li>
						}
						@if (favoritos != null)
						{
							<li class="nav-item">
								<h3 style="color:crimson">Empleados Favoritos: @favoritos.Count</h3>
							</li>
						}else{
							<li class="nav-item">
								<h3 style="color:crimson">Empleados Favoritos: 0</h3>
							</li>
						}
					</ul>
				</div>
			</div>
		</nav>
	</header>
	<div class="container">
		<main role="main" class="pb-3">
			@RenderBody()
		</main>
	</div>

	<footer class="border-top footer text-muted">
		<div class="container">
			&copy; 2025 - MvcNetCoreSessionEmpleados - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
		</div>
	</footer>
	<script src="~/lib/jquery/dist/jquery.min.js"></script>
	<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
	<script src="~/js/site.js" asp-append-version="true"></script>
	@await RenderSectionAsync("Scripts", required: false)
</body>
</html>
